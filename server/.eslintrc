{
  "extends": "eslint:recommended",

  "parser": "@typescript-eslint/parser",

  "parserOptions": {
    "sourceType": "module"
  },

  "plugins": ["@typescript-eslint"],

  "env": {
    "node": true
  },

  "globals": {
    "Promise": true
  },

  "rules": {
    "no-console": "off",
    "no-unused-vars": "off"
  }
}
